@model IEnumerable<GoodZoo.Models.Vet>

@{
    ViewData["Title"] = "Vet";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<a href="/"><i class="homeBtn fas fa-home fa-2x"></i></a>


<table class="table">
    <h1>current specialists</h1>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            Specialty
        </th>
        <th></th>
    </tr>

    @foreach (var listVet in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelVet => listVet.Name)
            </td>
            <td>
                @Html.DisplayFor(modelVet => listVet.Specialty)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = listVet.VetId }) |
                @Html.ActionLink("Details", "Details", new { id = listVet.VetId }) |
                @Html.ActionLink("Delete", "Delete", new { id = listVet.VetId })
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Create New", "Create")
</p>

@model GoodZoo.Models.Vet

@{
    ViewData["Title"] = "Vet - Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Vet Details</h2>
<hr />

<p>@Html.DisplayNameFor(model => model.Name): @Html.DisplayFor(model => model.Name)</p>
<p>@Html.DisplayNameFor(model => model.Specialty): @Html.DisplayFor(model => model.Specialty)</p>

<ul>
    @foreach (var animal in Model.Animals)
    {
    <li>@Html.DisplayFor(modelItem => animal.Name) | @Html.DisplayFor(modelItem => animal.Sex) | @Html.DisplayFor(modelItem => animal.Species) </li>

    }
</ul>


<p>@Html.ActionLink("Edit", "Edit", new { id = Model.VetId }) | @Html.ActionLink("Back to List", "Index")</p>


        }
